{
    "name": "Crypto Tracker",
    "description": "Track Crypto with a click!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "identity"
    ],

    "host_permissions": [
        "https://*/*"
    ],
    
    "action": {
        "default_popup": "popup/popup.html"
    }
}