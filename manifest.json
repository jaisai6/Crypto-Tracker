{
    "name": "Crypto Tracker",
    "description": "Track Crypto with a click!",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {        
        "128": "logo/logo128.png"            
      },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "identity",
        "alarms"
    ],

    "host_permissions": [
        "https://*/*"
    ],
    
    "action": {
        "default_popup": "popup/popup.html"
    }
}